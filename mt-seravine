            -- Copy to clipboard if possible
            if setclipboard then
                setclipboard("Vector3.new(" .. math.floor(pos.X) .. ", " .. math.floor(pos.Y) .. ", " .. math.floor(pos.Z) .. ")")
                print("üìã Position copied to clipboard!")
            end
        end
    end,
})

